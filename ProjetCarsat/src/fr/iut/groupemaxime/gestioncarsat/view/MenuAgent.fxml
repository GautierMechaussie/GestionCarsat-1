<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <children>
      <ScrollPane prefHeight="400.0" prefWidth="300.0" vbarPolicy="NEVER">
         <content>
            <VBox prefHeight="100.0" prefWidth="300.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label prefHeight="50.0" prefWidth="100.0" text="Limoges">
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </Label>
                        <Label prefHeight="50.0" prefWidth="200.0" text="Du 12/00/0000 au 00/00/0000" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0">
                     <children>
                        <Button mnemonicParsing="false" prefWidth="80.0" text="Afficher" />
                        <Button mnemonicParsing="false" prefWidth="80.0" text="Modifier" />
                        <Button mnemonicParsing="false" prefWidth="80.0" text="Supprimer" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
